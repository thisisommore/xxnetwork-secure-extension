{
  "name": "Security Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "16xxlogo.png",
      "48": "48xxlogo.png",
      "128": "128xxlogo.png"
    }
  },
  "externally_connectable": {
    "matches": ["http://localhost:3000/*"]
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "src/extension/service-worker.ts"
  }
}
