{
  "name": "Security Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "sx-logo.png",
      "48": "sx-logo.png",
      "128": "sx-logo.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:3000/*"],
      "js": ["content-script.ts"]
    }
  ],
  "permissions": ["storage"]
}
