{
  "name": "Security Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "xxlogo.png",
      "48": "xxlogo.png",
      "128": "xxlogo.png"
    }
  },
  "background": {
    "service_worker": "service-worker.ts",
    "type": "module"
  },
  "content_scripts": [ 
    {
      "matches": ["http://haven.xx.network/*"],
      "js": ["content-script.ts"]
    }
  ],
  "permissions": [
    "storage",
    "scripting",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
} 